<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>RegisterUser</servlet-name>
		<display-name>RegisterUser</display-name>
		<description></description>
		<servlet-class>Servlets.RegisterUser</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>getCertifiedDoctors</servlet-name>
		<display-name>getCertifiedDoctors</display-name>
		<description></description>
		<servlet-class>Servlets.getCertifiedDoctors</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateUserDetails</servlet-name>
		<display-name>UpdateUserDetails</display-name>
		<description></description>
		<servlet-class>Servlets.UpdateUserDetails</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>Servlets.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<display-name>Logout</display-name>
		<description></description>
		<servlet-class>Servlets.Logout</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>RegisterUser</servlet-name>
		<url-pattern>/RegisterUser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>getCertifiedDoctors</servlet-name>
		<url-pattern>/getCertifiedDoctors</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateUserDetails</servlet-name>
		<url-pattern>/UpdateUserDetails</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/Logout</url-pattern>
	</servlet-mapping>
	
</web-app>
